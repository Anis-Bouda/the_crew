import java.awt.Dimension;

public class NOT extends AbstractComponent {

    public NOT() {
        super("NOT");
        // Définir la taille spécifique du NOT
        setPreferredSize(new Dimension(100, 60));
        // Ajouter un port d'entrée et un port de sortie.
        // La position initiale est fictive, elle sera recalculée lors du repaint.
        inputPorts.add(new java.awt.Point(0, 0));
        outputPorts.add(new java.awt.Point(0, 0));
    }
    
    // Vous pouvez ajouter ici des méthodes spécifiques au comportement du NOT.
}
